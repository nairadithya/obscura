{
  "manifest_version": 3,
  "name": "Obscura",
  "host_permissions": ["*://*.instagram.com/"],
  "version": "0.1",
  "description": "An improved Instagram experience.",
  "permissions": ["webNavigation", "tabs", "activeTab"],
  "action": {
    "default_popup": "hello.html"
  },
  "background": {
    "scripts": ["background/backgroundRedirector.js"],
    "service_worker": "background/backgroundRedirector.js"
  }
}
