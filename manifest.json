{
  "manifest_version": 3,
  "name": "Instagram Reels Redirect",
  "host_permissions": ["*://*.instagram.com/"],
  "version": "1.0",
  "description": "Redirects Reels To Curb The Scrolling",
  "permissions": ["webNavigation", "tabs"],
  "action": {
    "default_popup": "hello.html"
  },
  "background": {
    "scripts" : ["background/backgroundRedirector.js"],
    "service_worker" : "background/backgroundRedirector.js"
  }
}
